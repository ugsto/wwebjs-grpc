{
	"name": "wwebjs-grpc",
	"version": "0.0.1",
	"description": "WhatsApp Web JS as a language-server agnostic gRPC server, facilitating integration across arious platforms and languages",
	"main": "index.ts",
	"type": "module",
	"scripts": {
		"start": "tsx src/index.ts",
		"start:dev": "tsx --watch src/index.ts",
		"build": "tsup src",
		"test": "vitest run",
		"prepare": "husky install",
		"proto": "tsx scripts/proto.ts"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/ugsto/wwebjs-grpc.git"
	},
